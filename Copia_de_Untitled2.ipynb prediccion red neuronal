    {
      "cell_type": "code",
      "source": [
        "# Importar bibliotecas necesarias\n",
        "import yfinance as yf\n",
        "import pandas as pd\n",
        "from sklearn.preprocessing import MinMaxScaler\n",
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import LSTM, Dense\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Paso 1: Obtener y Preprocesar los Datos\n",
        "# Descargar los datos de EUR/USD a intervalos de 5 minutos para un mes\n",
        "ticker = \"EURUSD=X\"  # Símbolo de EUR/USD\n",
        "start_date = \"2024-09-20\"\n",
        "end_date = \"2024-10-20\"\n",
        "\n",
        "# Descargar los datos\n",
        "datos = yf.download(ticker, start=start_date, end=end_date, interval='5m')\n",
        "\n",
        "# Seleccionar la columna \"Close\" para la predicción\n",
        "datos = datos[[\"Close\"]]\n",
        "\n",
        "# Verificar si los datos fueron obtenidos\n",
        "if datos.empty:\n",
        "    raise ValueError(\"No se obtuvieron datos para las fechas especificadas o el intervalo. Intenta con intervalos mayores o verifica la disponibilidad.\")\n",
        "\n",
        "# Escalar los datos entre 0 y 1 para LSTM\n",
        "scaler = MinMaxScaler(feature_range=(0, 1))\n",
        "datos_scaled = scaler.fit_transform(datos)\n",
        "\n",
        "# Paso 2: Crear secuencias de entrenamiento y etiquetas\n",
        "def crear_secuencias(datos, pasos):\n",
        "    X = []\n",
        "    y = []\n",
        "    for i in range(pasos, len(datos)):\n",
        "        X.append(datos[i-pasos:i, 0])\n",
        "        y.append(datos[i, 0])\n",
        "    return np.array(X), np.array(y)\n",
        "\n",
        "pasos = 60  # Número de intervalos de 5 minutos (5 horas de datos)\n",
        "X, y = crear_secuencias(datos_scaled, pasos)\n",
        "\n",
        "# Redimensionar los datos para que sean compatibles con LSTM (agregar una dimensión de características)\n",
        "X = np.expand_dims(X, axis=2)\n",
        "\n",
        "# Paso 3: Crear y Entrenar el Modelo LSTM\n",
        "modelo = Sequential()\n",
        "modelo.add(LSTM(50, return_sequences=True, input_shape=(X.shape[1], 1)))\n",
        "modelo.add(LSTM(50))\n",
        "modelo.add(Dense(1))\n",
        "\n",
        "modelo.compile(optimizer='adam', loss='mean_squared_error')\n",
        "modelo.fit(X, y, epochs=10, verbose=0)\n",
        "\n",
        "# Paso 4: Predecir las próximas horas\n",
        "predicciones = []\n",
        "periodo_prediccion = 12  # Número de horas a predecir\n",
        "\n",
        "# Usar la última secuencia como entrada para predecir las próximas horas\n",
        "ultima_secuencia = datos_scaled[-pasos:]\n",
        "\n",
        "for _ in range(periodo_prediccion):\n",
        "    entrada = np.expand_dims(ultima_secuencia, axis=0)\n",
        "    prediccion = modelo.predict(entrada)[0][0]\n",
        "    predicciones.append(prediccion)\n",
        "\n",
        "    # Actualizar la secuencia eliminando el primer valor y agregando la predicción\n",
        "    ultima_secuencia = np.vstack([ultima_secuencia[1:], [[prediccion]]])\n",
        "\n",
        "# Revertir las predicciones al rango original\n",
        "predicciones = scaler.inverse_transform(np.array(predicciones).reshape(-1, 1))\n",
        "\n",
        "# Paso 5: Crear un DataFrame con los resultados y el tiempo\n",
        "fechas_prediccion = pd.date_range(start=datos.index[-1], periods=periodo_prediccion+1, freq='H')[1:]\n",
        "predicciones_df = pd.DataFrame(predicciones, columns=[\"Predicción\"], index=fechas_prediccion)\n",
        "\n",
        "# Combinar los datos reales con las predicciones\n",
        "datos_reales = datos[-60:]  # Últimas 60 observaciones (5 horas)\n",
        "resultado_combinado = pd.concat([datos_reales, predicciones_df])\n",
        "\n",
        "# Paso 6: Visualizar las Predicciones y el Detalle Numérico\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(datos_reales.index, datos_reales[\"Close\"], label=\"Valores Reales\", color='blue')\n",
        "plt.plot(predicciones_df.index, predicciones_df[\"Predicción\"], label=\"Predicciones\", color='red', linestyle='dashed')\n",
        "plt.title(\"Predicción de EUR/USD en Intervalos de 1 Hora\")\n",
        "plt.xlabel(\"Tiempo\")\n",
        "plt.ylabel(\"Precio de Cierre\")\n",
        "plt.legend()\n",
        "plt.grid()\n",
        "plt.show()\n",
        "\n",
        "# Mostrar el detalle numérico de las predicciones\n",
        "print(\"Detalle Numérico de las Predicciones:\")\n",
        "print(predicciones_df)\n"
      ],
      "metadata": {
