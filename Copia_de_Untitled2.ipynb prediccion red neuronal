      "cell_type": "code",
      "source": [
        "import matplotlib.pyplot as plt\n",
        "import pandas as pd\n",
        "\n",
        "# Valores reales (datos observados hasta la última hora disponible)\n",
        "valores_reales = datos[['Close']]  # Usamos la columna de cierre real de los datos descargados\n",
        "\n",
        "# Definir las fechas predichas\n",
        "# Supongamos que tenemos `predicciones` y que el número de predicciones es el mismo que el rango de horas futuras deseado\n",
        "ultima_fecha_real = valores_reales.index[-1]\n",
        "fechas_predichas = pd.date_range(start=ultima_fecha_real + pd.Timedelta(hours=1), periods=len(predicciones), freq='H')\n",
        "\n",
        "# Crear una Serie para las predicciones futuras con las fechas predichas como índice\n",
        "predicciones_futuras = pd.Series(predicciones[:, 0], index=fechas_predichas)\n",
        "\n",
        "# Crear el gráfico comparativo\n",
        "plt.figure(figsize=(12, 6))\n",
        "plt.plot(valores_reales.index, valores_reales['Close'], label=\"Valores Reales\", color='blue')\n",
        "plt.plot(predicciones_futuras.index, predicciones_futuras, label=\"Predicciones Futuras\", color='red', linestyle='--')\n",
        "\n",
        "# Configuraciones del gráfico\n",
        "plt.title(\"Comparación de Tendencia Real y Proyección Futura de EUR/USD\")\n",
        "plt.xlabel(\"Tiempo\")\n",
        "plt.ylabel(\"Precio de Cierre\")\n",
        "plt.legend()\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# Generar el informe de datos numéricos\n",
        "print(\"Informe de Predicciones por Hora para las Próximas Horas:\")\n",
        "informe_predicciones = pd.DataFrame({\n",
        "    \"Hora\": fechas_predichas,\n",
        "    \"Precio Predicho\": predicciones[:, 0]\n",
        "})\n",
        "print(informe_predicciones)\n"
      ],
